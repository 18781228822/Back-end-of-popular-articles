<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.swjtuhc.demo.mapper.ComplaintsMapper">
	<insert id="inserInformation" parameterType="edu.swjtuhc.demo.model.ComplaintsModel">
		insert into complaints
		(username,content)
		values
		(#{username},#{content})
	</insert>
</mapper>